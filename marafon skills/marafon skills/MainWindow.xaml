<Window x:Class="marafon_skills.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:marafon_skills"
        mc:Ignorable="d"
        Title="Marathon Skills 2017" Style="{DynamicResource base_window}" Height="500" Width="700">
    <Grid Margin="0,0,0,-6">
        
        <Rectangle Fill="{DynamicResource dark_grey }" Height="52" VerticalAlignment="Top" Margin="0,428,0,0" RenderTransformOrigin="0.494,0.482" />
        <Label x:Name="label_timer" Foreground="White" Content="{Binding Time, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" VerticalContentAlignment="Top" HorizontalContentAlignment="Center" Height="44" Width="467" Margin="116,432,116,14"/>
        <Frame x:Name="frame" Content="Frame" Margin="0,34,0,10" Source="/MainPage.xaml" NavigationUIVisibility="Hidden" Navigated="frame_Navigated" />

    </Grid>
</Window>
